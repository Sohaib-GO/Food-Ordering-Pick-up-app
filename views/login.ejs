<html lang="en" >
<body>
  <header>
    <%- include('partials/_header') %>
  </header>
<head>

  <meta charset="UTF-8">
  <title>login page</title>
  <link href="https://fonts.googleapis.com/css?family=Assistant:400,700" rel="stylesheet">
  <link rel="stylesheet" href="/styles/login.css">

</head>
  <section class='login' id='login'>
    <div class='head'>
    <h1 class='company'>LOGIN</h1>
    </div>
    <div class='form'>
    <form action="/login" method="POST">
    <p class="username_holder">Username</p>
    <input type="text" placeholder='Type your username' class='text' id='username' name="email" required><br>
    <p class="password_holder">Password</p>
    <input type="password" placeholder='Type your password' class='password' name="password"><br>
    <button class='btn-login' type="submit">Login</button>
      </form>
    </div>
  </section>
  </body>
</html>

